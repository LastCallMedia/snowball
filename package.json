{
  "name": "snowball-storybook-2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/manager-api": "^7.2.0",
    "@storybook/theming": "^7.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006 -s ../public",
    "build-storybook": "storybook build -s public",
    "build-storybook-for-github-pages": "storybook build -o docs-build -s public",
    "watchcss": ""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@lastcall/tailwind-snowball": "^0.0.3",
    "@storybook/addon-actions": "^7.2.0",
    "@storybook/addon-essentials": "^7.2.0",
    "@storybook/addon-interactions": "^7.2.0",
    "@storybook/addon-links": "^7.2.0",
    "@storybook/addon-mdx-gfm": "^7.2.0",
    "@storybook/node-logger": "^7.2.0",
    "@storybook/preset-create-react-app": "^7.2.0",
    "@storybook/react": "^7.2.0",
    "@storybook/react-webpack5": "^7.2.0",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/typography": "^0.5.9",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.2.0",
    "tailwindcss": "^3.2.7",
    "webpack": "^5.76.2"
  }
}
